{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=playwrightservice;Username=playwright_user;Password=playwright_password"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Jwt": {
    "SecretKey": "your-super-secret-key-that-is-at-least-32-characters-long",
    "Issuer": "PlaywrightService",
    "Audience": "PlaywrightService",
    "ExpiryMinutes": 60
  },
  "Playwright": {
    "Headless": true,
    "Timeout": 30000,
    "ScreenshotPath": "./screenshots",
    "VideoPath": "./videos"
  },
  "AI": {
    "OpenAI": {
      "ApiKey": "your-openai-api-key",
      "Model": "gpt-4",
      "MaxTokens": 4000
    }
  }
}
