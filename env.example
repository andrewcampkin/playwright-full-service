# Environment Configuration Example
# Copy this file to .env and update the values

# Database Configuration
POSTGRES_DB=playwrightservice
POSTGRES_USER=playwright_user
POSTGRES_PASSWORD=playwright_password

# Redis Configuration
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-key-that-is-at-least-32-characters-long
JWT_ISSUER=PlaywrightService
JWT_AUDIENCE=PlaywrightService
JWT_EXPIRY_MINUTES=60

# AI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000

# Playwright Configuration
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000
PLAYWRIGHT_SCREENSHOT_PATH=./screenshots
PLAYWRIGHT_VIDEO_PATH=./videos

# Frontend Configuration
VITE_API_URL=http://localhost:5000
VITE_API_BASE_URL=http://localhost:5000/api

# Development Configuration
ASPNETCORE_ENVIRONMENT=Development
NODE_ENV=development
